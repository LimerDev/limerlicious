{
	"$schema": "vscode://schemas/color-theme",
	"name": "Liljam Dark",
	"editor.lineHeight": 1.5,
	"colors": {
		"activityBar.background": "#303030",
		"activityBar.border": "#252525",
		"activityBar.foreground": "#bfbfbf",
		"activityBarBadge.background": "#222222",
		"activityBarBadge.foreground": "#bdbdbd",
		"debugIcon.startForeground": "#6fcf62",
		"editor.background": "#2b2b2b",
		"editor.foldBackground": "#272727",
		"editor.lineHighlightBackground": "#252525",
		"editor.lineHighlightBorder": "#00000000",
		"editor.selectionBackground": "#19252b",
		"editorBracketMatch.background": "#19252b",
		"editorBracketMatch.border": "#19252b",
		"editorIndentGuide.background1": "#25252500",
		"editorStickyScroll.background": "#252525",
		"editorStickyScroll.shadow": "#fafafa00",
		"editorSuggestWidget.background": "#272727",
		"editorSuggestWidget.border": "#1d1d1d",
		"editorSuggestWidget.selectedBackground": "#20323a",
		"editorWidget.foreground": "#aaaaaa",
		"focusBorder": "#2b2b2b2b",
		"foreground": "#bdbdbd",
		"gitDecoration.addedResourceForeground": "#98c78e",
		"gitDecoration.modifiedResourceForeground": "#98c78e",
		"gitDecoration.untrackedResourceForeground": "#98c78e",
		"icon.foreground": "#656565",
		"list.activeSelectionBackground": "#303030",
		"list.activeSelectionForeground": "#c9d4d6",
		"list.errorForeground": "#eb6e6e",
		"list.focusAndSelectionOutline": "#ff000000",
		"list.focusBackground": "#303030",
		"list.focusForeground": "#c9d4d6",
		"list.hoverBackground": "#3f3f3f",
		"list.hoverForeground": "#c9d4d6",
		"list.inactiveSelectionBackground": "#272727",
		"list.inactiveSelectionForeground": "#c9d4d6",
		"menubar.selectionBorder": "#252525",
		"panel.border": "#252525",
		"panel.background" : "#2b2b2b",
		"peekView.border": "#252525",
		"peekViewEditor.background": "#363636",
		"peekViewEditorStickyScroll.background": "#252525",
		"peekViewResult.background": "#303030",
		"peekViewResult.fileForeground": "#9ea2a3",
		"peekViewResult.lineForeground": "#818d8f",
		"peekViewTitle.background": "#303030",
		"scmGraph.historyItemRefColor": "#5b6e57",
		"scmGraph.historyItemRemoteRefColor": "#5b6e57",
		"scrollbar.shadow": "#00000000",
		"sideBar.background": "#303030",
		"sideBar.border": "#252525",
		"statusBar.background": "#303030",
		"statusBar.foreground": "#bdbdbd",
		"tab.activeBackground": "#2b2b2b",
		"tab.activeForeground": "#bdbdbd",
		"tab.border": "#252525",
		"tab.hoverBackground": "#2b2b2b",
		"tab.inactiveBackground": "#202020",
		"tab.inactiveForeground": "#bdbdbd",
		"terminal.border": "#252525",
		"terminal.foreground": "#6e6e6e",
		"testing.iconUnset": "#575757",
		"testing.runAction": "#6fcf62",
		"titleBar.activeBackground": "#303030",
		"titleBar.border": "#252525",
		"tree.inactiveIndentGuidesStroke": "#11111100",
		"tree.indentGuidesStroke": "#00000000",
	},
	"tokenColors": [
		{
			"scope": [
				"meta",
				"meta.embedded",
				"source.groovy.embedded",
				"string meta.image.inline.markdown",
				"variable.legacy.builtin.python"
			],
			"settings": {
				"foreground": "#D4D4D4"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#000080"
			}
		},
		{
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#567e5b"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#54a2c0"
			}
		},
		{
			"scope": [
				"constant.numeric",
				// "variable.other.enummember",
				"keyword.operator.plus.exponent",
				"keyword.operator.minus.exponent"
			],
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#646695"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#54a2c0"
			}
		},
		{
			"scope": [
				"entity.name.tag.css",
				"entity.name.tag.less"
			],
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.class.css",
				"source.css entity.other.attribute-name.class",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.parent.less",
				"source.css entity.other.attribute-name.pseudo-class",
				"entity.other.attribute-name.pseudo-element.css",
				"source.css.less entity.other.attribute-name.id",
				"entity.other.attribute-name.scss"
			],
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#54a2c0"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#54a2c0"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#54a2c0"
			}
		},
		{
			"scope": "punctuation.definition.quote.begin.markdown",
			"settings": {
				"foreground": "#6A9955"
			}
		},
		{
			"scope": "punctuation.definition.list.begin.markdown",
			"settings": {
				"foreground": "#6796e6"
			}
		},
		{
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#bdbdbd"
			}
		},
		{
			"scope": [
				"meta.preprocessor",
				"entity.name.function.preprocessor"
			],
			"settings": {
				"foreground": "#54a2c0"
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"scope": "meta.preprocessor.numeric",
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"scope": "meta.structure.dictionary.key.python",
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#54a2c0"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#54a2c0"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#54a2c0"
			}
		},
		{
			"scope": [
				"storage.modifier",
				"keyword.operator.noexcept"
			],
			"settings": {
				"foreground": "#54a2c0"
			}
		},
		{
			"scope": [
				"string",
				"meta.embedded.assembly"
			],
			"settings": {
				"foreground": "#c9866c"
			}
		},
		{
			"scope": "string.tag",
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"scope": "string.value",
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#d16969"
			}
		},
		{
			"name": "String interpolation",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#54a2c0"
			}
		},
		{
			"name": "Reset JavaScript string interpolation expression",
			"scope": [
				"meta.template.expression"
			],
			"settings": {
				"foreground": "#D4D4D4"
			}
		},
		{
			"scope": [
				"support.type.vendored.property-name",
				"support.type.property-name",
				"source.css variable",
				"source.coffee.embedded"
			],
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#368fb8"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#d4d4d4"
			}
		},
		{
			"scope": [
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.alignof",
				"keyword.operator.typeid",
				"keyword.operator.alignas",
				"keyword.operator.instanceof",
				"keyword.operator.logical.python",
				"keyword.operator.wordlike"
			],
			"settings": {
				"foreground": "#54a2c0"
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"scope": [
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php"
			],
			"settings": {
				"foreground": "#54a2c0"
			}
		},
		{
			"scope": "support.function.git-rebase",
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"scope": "constant.sha.git-rebase",
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"name": "coloring of the Java import and package identifiers",
			"scope": [
				"storage.modifier.import.java",
				"variable.language.wildcard.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"foreground": "#d4d4d4"
			}
		},
		{
			"name": "this.self",
			"scope": "variable.language",
			"settings": {
				"foreground": "#54a2c0"
			}
		},
		{
			"name": "other",
			"scope": [
				"entity",
				"variable"
			],
			"settings": {
				"foreground": "#cfcfcf"
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"class": "#48cba4",
		"constant": "#c7c7c7",
		"controlKeyword": "#be8ec0",
		"enum": "#bac79c",
		"interface": "#bac79c",
		"jsonPropertyName": "#89bed6",
		"method": "#ccd49b",
		"number": "#a6c79c",
		"struct": "#77b990",
		"variable": "#8fc4dd",
		// "enumMember": "#d4d4d4",
	}
}